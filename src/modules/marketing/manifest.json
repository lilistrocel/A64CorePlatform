{
  "module_name": "marketing",
  "display_name": "Marketing",
  "version": "1.0.0",
  "description": "Marketing management system for campaigns, budgets, channels, and events",
  "route_prefix": "/api/v1/marketing",
  "dependencies": {
    "core": ">=1.3.0",
    "python": ">=3.11"
  },
  "database_collections": [
    "marketing_budgets",
    "marketing_campaigns",
    "marketing_channels",
    "marketing_events"
  ],
  "permissions": [
    "marketing.view",
    "marketing.create",
    "marketing.edit",
    "marketing.delete"
  ],
  "license_required": false,
  "enabled_by_default": true,
  "core_module": false
}
