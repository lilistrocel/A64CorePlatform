{
  "module_name": "crm",
  "display_name": "CRM",
  "version": "1.0.0",
  "description": "Customer Relationship Management system for managing customers, contacts, and business relationships",
  "route_prefix": "/api/v1/crm",
  "dependencies": {
    "core": ">=1.3.0",
    "python": ">=3.11"
  },
  "database_collections": [
    "customers"
  ],
  "permissions": [
    "crm.view",
    "crm.create",
    "crm.edit",
    "crm.delete"
  ],
  "license_required": false,
  "enabled_by_default": true,
  "core_module": false
}
