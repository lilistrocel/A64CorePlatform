{
  "module_name": "logistics",
  "display_name": "Logistics",
  "version": "1.0.0",
  "description": "Logistics management system for vehicles, routes, and shipments",
  "route_prefix": "/api/v1/logistics",
  "dependencies": {
    "core": ">=1.3.0",
    "python": ">=3.11"
  },
  "database_collections": [
    "vehicles",
    "routes",
    "shipments"
  ],
  "permissions": [
    "logistics.view",
    "logistics.create",
    "logistics.edit",
    "logistics.delete"
  ],
  "license_required": false,
  "enabled_by_default": true,
  "core_module": false
}
